macro_xawx: macro_xawx{
compatible = "zmk,behavior-macro";
label = "macro_xawx";
#binding-cells = <0>;
bindings 
  = <&macro_press &kp LEFT_ALT &kp LMETA>
  , <&macro_pause_for_release>
  , <&macro_release &kp LEFT_ALT &kp LMETA>
  ;
};
